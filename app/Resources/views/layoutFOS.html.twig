<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8" />
	<title>{% block title %}MMI TV 3 !{% endblock %}</title>
	{% stylesheets 'bundles/mmimmi/css/style.css' filter='cssrewrite' %}
	<link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %}
	<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
	{% javascripts '@MMIMMIBundle/Resources/public/js/jquery-1.11.0.min.js' %}
	<script type="text/javascript" src="{{ asset_url }}"></script>
	{% endjavascripts %}
	{% javascripts '@MMIMMIBundle/Resources/public/js/bootstrap.js' %}
	<script type="text/javascript" src="{{ asset_url }}"></script>
	{% endjavascripts %}
</head>
<body>
	<div>
	</div>

	{% for type, messages in app.session.flashBag.all %}
	{% for message in messages %}
	<div class="{{ type }}">
		{{ message|trans({}, 'FOSUserBundle') }}
	</div>
	{% endfor %}
	{% endfor %}

	<div>
		{% block fos_user_content %}
		{% endblock fos_user_content %}
	</div>
</body>
</html>