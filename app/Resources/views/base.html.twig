  {% stylesheets 'bundles/mmimmi/css/style.css' filter='cssrewrite' %}
  <link rel="stylesheet" href="{{ asset_url }}" />
  {% endstylesheets %}

  {% stylesheets 'bundles/mmimmi/css/fullcalendar.css' filter='cssrewrite' %}
  <link rel="stylesheet" href="{{ asset_url }}" />
  {% endstylesheets %}

  {% stylesheets 'bundles/mmimmi/css/fullcalendar.min.css' filter='cssrewrite' %}
  <link rel="stylesheet" href="{{ asset_url }}" />
  {% endstylesheets %}

  {% stylesheets 'bundles/mmimmi/css/fullcalendar.print.css' filter='cssrewrite' %}
  <link rel="stylesheet" href="{{ asset_url }}" />
  {% endstylesheets %}

  {% stylesheets 'bundles/mmimmi/css/calendrier_perso.css' filter='cssrewrite' %}
  <link rel="stylesheet" href="{{ asset_url }}" />
  {% endstylesheets %}


<link rel="stylesheet" href="{{ asset('bundles/adesignscalendar/css/fullcalendar/fullcalendar.css') }}" />


  <link rel="icon" type="image/x-icon" href="{{ asset('bundles/mmimmi/images/logoi.png') }}" />
  {% javascripts '@MMIMMIBundle/Resources/public/js/jquery-1.11.0.min.js' %}
  <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}

  {% javascripts '@MMIMMIBundle/Resources/public/js/bootstrap.js' %}
  <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}

  {% javascripts '@MMIMMIBundle/Resources/public/js/fullcalendar.min.js' %}
  <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}

  {% javascripts '@MMIMMIBundle/Resources/public/js/jquery.min.js' %}
  <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}

  {% javascripts '@MMIMMIBundle/Resources/public/js/jquery-ui.custom.min.js' %}
  <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}

  {% javascripts '@MMIMMIBundle/Resources/public/js/moment.min.js' %}
  <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}

  {% javascripts '@MMIMMIBundle/Resources/public/js/calendrier_perso.js' %}
  <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}

{% javascripts %}
<script type="text/javascript" src="{{ asset('bundles/adesignscalendar/js/jquery/jquery-1.8.2.min.js') }}"></script> {% endjavascripts %}

{% javascripts %}
<script type="text/javascript" src="{{ asset('bundles/adesignscalendar/js/fullcalendar/jquery.fullcalendar.min.js') }}"></script> {% endjavascripts %}

{% javascripts %}
<script type="text/javascript" src="{{ asset('bundles/adesignscalendar/js/calendar-settings.js') }}"></script> {% endjavascripts %}

{% include 'ADesignsCalendarBundle::calendar.html.twig' %}






    <link href='{{ asset('bundles/mmimmi/fullcalendar/fullcalendar.css') }}' rel='stylesheet' />
    <link href='{{ asset('bundles/mmimmi/fullcalendar/fullcalendar.print.css') }}' rel='stylesheet' media='print' />




<style>


    #wrap {
        width: 1100px;
        margin: 0 auto;
          margin-top: 3%;
    }

    #external-events {
        float: left;
        width: 180px;
        padding: 0 10px;
        border: 1px solid #ccc;
        background: #eee;
        text-align: left;
    }

    #external-events h4 {
        font-size: 16px;
        margin-top: 0;
        padding-top: 1em;
    }

    #external-events .fc-event {
        margin: 10px 0;
        cursor: pointer;
    }

    #external-events p {
        margin: 1.5em 0;
        font-size: 11px;
        color: #666;
    }

    #external-events p input {
        margin: 0;
        vertical-align: middle;
    }

    #calendar {
        float: right;
        color: #FFF;
    }
    .fc-toolbar h2 {
  margin: 0;
  text-transform: uppercase;
  color: #FFF !important;
}

.fc-event {
  position: relative;
  display: block;
  font-size: .85em;
  line-height: 1.3;
  border-radius: 3px;
  border: 1px solid #52be9c;
  background-color: #52be9c;
  font-weight: normal;
}

.fc-unthemed .fc-today {
  background: #52be9c;
}

</style>








</head>
<body>
  {% block body %}
  <nav class="navbar navbar-inverse">
    <div class="container-fluid">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-2">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a href="{{ path('mmi_accueil') }}"><img class="img1" src="{{ asset('bundles/mmimmi/images/logo.png') }}" alt="logo"></a>
      </div>

      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-2">
      {% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
        <ul class="nav navbar-nav">
          <li><a href="{{ path('mmi_agenda') }}">Agenda</a></li>
          <li><a href="{{ path('mmi_news') }}">News</a></li>
          <li><a href="{{ path('evenements') }}">Événements</a></li>
          <li><a href="{{ path('mmi_portfolio') }}">Portfolio</a></li>
          <li><a href="{{ path('mmi_videos') }}">Vidéos</a></li>
          <li><a href="{{ path('mmi_grille') }}">Grille</a></li>        
          <li><a href="{{path('mmi_direct')}}">Direct</a></li>
        </ul>
        {% endif %}
        <ul class="nav navbar-nav navbar-right">
          {% if is_granted('ROLE_ADMIN') %}
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Administrateur <span class="caret"></span></a>
            <ul class="dropdown-menu" role="menu">
              <li><a href="{{ path('fos_user_registration_register') }}">Ajouter un utilisateur</a></li>
            </ul>
          </li>
          {% endif %}
          {% if app.user %}
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Bonjour {{ app.user.username }} ! <span class="caret"></span></a>
            <ul class="dropdown-menu" role="menu">
               <li><a href="{{ path('fos_user_security_logout') }}">Deconnexion</a></li>
            </ul>
          </li>
          <li><a href="#"></a></li>
          {% else %}
          <li><a href="{{ path('fos_user_security_login') }}">Connectez-vous</a></li>

          {% endif %}
        </ul>
      </div>
    </div>
  </nav>
  {% endblock %}
</body>
</html>
